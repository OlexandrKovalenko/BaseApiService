{
  "name": "baseapi/apibase",
  "description": "Base Api",
  "type": "project",
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "monolog/monolog": "^2",
    "vlucas/phpdotenv": "^5.0",
    "phpunit/phpunit": "^9.5",
    "firebase/php-jwt": "^6.0",
    "robmorgan/phinx": "^0.12.6",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.5"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
